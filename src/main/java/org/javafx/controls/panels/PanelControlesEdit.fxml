<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tooltip?>

<fx:root type="javafx.scene.layout.Pane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<HBox >
		<children>
			<Button fx:id="btnGuardar" >
				<tooltip><Tooltip text="Guardar Elemento"></Tooltip></tooltip>
			</Button>
		</children>
		<children>
			<Button fx:id="btnCancelar" >
				<tooltip><Tooltip text="Cancelar Elemento"></Tooltip></tooltip>
			</Button>
		</children>		
	</HBox>
</fx:root>
